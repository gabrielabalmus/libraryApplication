{
  "extends": "tsconfig/remix.json",
  "include": ["."],
  "exclude": ["dist", "build", "node_modules"],
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/components/*": ["./components/*"],
      "@/templates/*": ["./templates/*"],
      "@/utils/*": ["./utils/*"],
      "@/theme/*": ["./theme/*"],
    },
  }
}
